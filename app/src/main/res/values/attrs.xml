<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="reflect_relativeDepth" format="float"/>
    <attr name="reflect_gap" format="dimension"/>

    <declare-styleable name="ReflectionViewContainer">
        <attr name="reflect_relativeDepth"/>
        <attr name="reflect_gap"/>
    </declare-styleable>

    <declare-styleable name="ReflectionViewContainer_Layout">
        <attr name="reflect_relativeDepth"/>
        <attr name="reflect_gap"/>
    </declare-styleable>

    <attr name="reflect_reflectionLayoutStyle" format="reference" />

    <declare-styleable name="BubbleToggleView">
        <attr name="bt_icon" format="reference" />
        <attr name="bt_iconWidth" format="dimension" />
        <attr name="bt_iconHeight" format="dimension" />
        <attr name="bt_shape" format="reference" />
        <attr name="bt_shapeColor" format="color" />
        <attr name="bt_showShapeAlways" format="boolean" />
        <attr name="bt_title" format="string" />
        <attr name="bt_titleSize" format="dimension" />
        <attr name="bt_colorActive" format="color" />
        <attr name="bt_colorInactive" format="color" />
        <attr name="bt_active" format="boolean" />
        <attr name="bt_duration" format="integer" />
        <attr name="bt_padding" format="dimension" />
        <attr name="bt_titlePadding" format="dimension" />
        <attr name="bt_badgeTextSize" format="dimension" />
        <attr name="bt_badgeBackgroundColor" format="color" />
        <attr name="bt_badgeTextColor" format="color" />
        <attr name="bt_badgeText" format="string" />
    </declare-styleable>
    <declare-styleable name="BubbleNavigationConstraintView">
        <attr name="bnc_mode">
            <enum name="spread" value="0" />
            <enum name="inside" value="1" />
            <enum name="packed" value="2" />
        </attr>
    </declare-styleable>

    <!-- Reference to a style for the menu drawer. -->
    <attr name="elasticDrawerStyle" format="reference" />

    <!-- Styleables used for styling the menu drawer. -->
    <declare-styleable name="ElasticDrawer">

        <!-- Drawable to use for the background of the menu. -->
        <attr name="edMenuBackground" format="color" />

        <!-- The size of the menu. -->
        <attr name="edMenuSize" format="dimension" />

        <!-- The touch bezel size. -->
        <attr name="edTouchBezelSize" format="dimension" />

        <!-- The maximum animation duration -->
        <attr name="edMaxAnimationDuration" format="integer" />

        <!-- The position of the drawer -->
        <attr name="edPosition" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CropImageView">
        <attr name="cropGuidelines">
            <enum name="off" value="0"/>
            <enum name="onTouch" value="1"/>
            <enum name="on" value="2"/>
        </attr>
        <attr name="cropScaleType">
            <enum name="fitCenter" value="0"/>
            <enum name="center" value="1"/>
            <enum name="centerCrop" value="2"/>
            <enum name="centerInside" value="3"/>
        </attr>
        <attr name="cropShape">
            <enum name="rectangle" value="0"/>
            <enum name="oval" value="1"/>
        </attr>
        <attr name="cropAutoZoomEnabled" format="boolean"/>
        <attr name="cropMaxZoom" format="integer"/>
        <attr name="cropMultiTouchEnabled" format="boolean"/>
        <attr name="cropFixAspectRatio" format="boolean"/>
        <attr name="cropAspectRatioX" format="integer"/>
        <attr name="cropAspectRatioY" format="integer"/>
        <attr name="cropInitialCropWindowPaddingRatio" format="float"/>
        <attr name="cropBorderLineThickness" format="dimension"/>
        <attr name="cropBorderLineColor" format="color"/>
        <attr name="cropBorderCornerThickness" format="dimension"/>
        <attr name="cropBorderCornerOffset" format="dimension"/>
        <attr name="cropBorderCornerLength" format="dimension"/>
        <attr name="cropBorderCornerColor" format="color"/>
        <attr name="cropGuidelinesThickness" format="dimension"/>
        <attr name="cropGuidelinesColor" format="color"/>
        <attr name="cropBackgroundColor" format="color"/>
        <attr name="cropSnapRadius" format="dimension"/>
        <attr name="cropTouchRadius" format="dimension"/>
        <attr name="cropSaveBitmapToInstanceState" format="boolean"/>
        <attr name="cropShowCropOverlay" format="boolean"/>
        <attr name="cropShowProgressBar" format="boolean"/>
        <attr name="cropMinCropWindowWidth" format="dimension"/>
        <attr name="cropMinCropWindowHeight" format="dimension"/>
        <attr name="cropMinCropResultWidthPX" format="float"/>
        <attr name="cropMinCropResultHeightPX" format="float"/>
        <attr name="cropMaxCropResultWidthPX" format="float"/>
        <attr name="cropMaxCropResultHeightPX" format="float"/>
        <attr name="cropFlipHorizontally" format="boolean"/>
        <attr name="cropFlipVertically" format="boolean"/>
    </declare-styleable>

</resources>